{
  "name": "@korylee/utils",
  "version": "0.0.5",
  "description": "kory utils",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/index.d.ts"
    },
    "./lib/*": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/korylee/kr-utils"
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "prepublish": "vite build && tsc -p tsconfig.build.json",
    "jest": "jest --config jestconfig.json",
    "test": "vitest"
  },
  "author": "korylee <kory.lee@qq.com>",
  "license": "MIT",
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "kory-utils"
  ],
  "devDependencies": {
    "@vitest/ui": "^0.16.0",
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vite": "^2.9.13",
    "vitest": "^0.16.0"
  }
}
